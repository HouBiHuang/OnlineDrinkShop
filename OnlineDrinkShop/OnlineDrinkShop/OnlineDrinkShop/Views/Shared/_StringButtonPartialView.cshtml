@model string

<td style="width: 150px">
    <div class="btn-group">
        <a asp-action="Edit" asp-route-id="@Model" class="btn btn-primary"><i class="bi bi-pencil-square"></i></a>
        <a asp-action="Details" asp-route-id="@Model" class="btn btn-success"><i class="bi bi-card-list"></i></a>
        <a asp-action="Lockout" asp-route-id="@Model" class="btn btn-danger"><i class="bi bi-trash-fill"></i></a>
    </div>
</td>